<%= render "layouts/errors", :object => @post %>


<%= form_for [@post] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, size: "50x50" %>
  <br>
  <%= f.label :category %>
  <%= f.text_field :category, size: "50x50" %>
  <br>
  <%= f.label :body %>
  <%= f.text_field :body, size: "100x100" %>
  <br>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <br>
  <%= f.submit %>
<% end %>
